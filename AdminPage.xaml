<Window x:Class="Course_Lena.AdminPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Course_Lena"
        mc:Ignorable="d"
        Title="AdminPage" Height="175" Width="200">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition Height="1.25*" />
            <RowDefinition Height="1.25*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <TextBox Name="login" Margin="10" Grid.Row="0"></TextBox>
        <TextBox Name="password"  Margin="10" Grid.Row="1" AcceptsReturn="True" />
        <Button Grid.Row="2" Margin="10" HorizontalAlignment="Center" Width="50" Click="Button_Click"></Button>
    </Grid>
</Window>
